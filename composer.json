{
    "name": "ivan/php_webcrwler",
    "description": "Projeto de exemplo de como usar o composer",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "IvanRezini",
        "email": "ivan_resini@estudante.sc.senai.br"
    }],
    "require": {
        "guzzlehttp/guzzle": "^6.3",
        "symfony/dom-crawler": "^4.3",
        "symfony/css-selector": "^4.3",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "SENAI\\BuscadorDeCursos\\": "src/"
        },
        "scripts": {
            "cs": "phpcs --standard=PSR12 src/",
            "cbf": "phpcbf --standard=PSR12 src/"
        },
        "scripts-description": {
            "cs": "Verifica erros no padrao PSR12",
            "cbf": "Corige erros no padrao PSR12"
        }
    }
}